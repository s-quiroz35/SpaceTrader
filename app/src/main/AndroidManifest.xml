<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.cs2340.willcodeforfood.spacetrader">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.CargoActivity"></activity>
        <activity android:name=".View.ConfigActivity"></activity>
        <activity android:name=".View.ViewAllGamesActivity"></activity>
        <activity android:name=".View.UniverseActivity"></activity>
        <activity android:name=".View.PlanetActivity"></activity>
        <activity android:name=".View.MarketActivity"></activity>
        <activity android:name=".View.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>